{
    "name": "helgesverre/brain",
    "description": "The Adorably Simple OpenAI Wrapper",
    "keywords": [
        "openai",
        "ai",
        "wrapper",
        "brain"
    ],
    "homepage": "https://github.com/helgesverre/brain",
    "license": "MIT",
    "authors": [
        {
            "name": "Helge Sverre",
            "email": "helge.sverre@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "openai-php/laravel": "^v0.11.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "nunomaduro/collision": "^8.0|^9.0",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^2.20|^3.0|^4.0",
        "pestphp/pest-plugin-arch": "^2.0|^3.0|^4.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "HelgeSverre\\Brain\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HelgeSverre\\Brain\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint",
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HelgeSverre\\Brain\\ServiceProvider"
            ],
            "aliases": {
                "Brain": "HelgeSverre\\Brain\\Facades\\Brain"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
